APP_DEBUG = true[APP]DEFAULT_TIMEZONE = Asia/Shanghai[DATABASE]TYPE = mysqlHOSTNAME = 127.0.0.1DATABASE = quickcodeUSERNAME = rootPASSWORD = rootHOSTPORT = 3306CHARSET = utf8mb4DEBUG = true[LANG]default_lang = zh-cn[cache]DRIVER = redisstores-redis-host = 127.0.0.1stores-redis-password = 111111[applet]driver = wechatdrivers-wechat-appId = ''drivers-wechat-appSecret = ''[oss]driver = tencentdrivers-tencent-secretId = ''drivers-tencent-secretKey = '';sts token 过期时间 秒drivers-tencent-durationSeconds = 1800drivers-tencent-region = ap-shanghai;默认的存储桶 名称drivers-tencent-default_bucket = '';允许的操作类型drivers-tencent-allowActions = 'name/cos:PutObject','name/cos:PostObject'[pay]driver = wechatdrivers-wechat-app_id = '';商户iddrivers-wechat-mch_id = '';商户平台的api keydrivers-wechat-key = ''; 证书路径，退款需要指定绝对路径drivers-wechat-cert_path = ''; 证书路径，退款需要指定绝对路径drivers-wechat-key_path = ''[sms]driver = alibabadrivers-alibaba-accessKeyId= ''drivers-alibaba-accessKeySecret= ''drivers-alibaba-signName= ''drivers-tencent-secretId= ''drivers-tencent-secretKey= ''drivers-tencent-smsSdkAppId= ''drivers-tencent-signName= ''